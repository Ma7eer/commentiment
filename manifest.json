{
  "manifest_version": 2,

  "name": "Commentiment",
  "version": "0.1.0",
  "description": "Gets current youtube page's videoId",
  "permissions": [
    "tabs",
    "activeTab",
    "*://*/sentiment/*"
  ],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  }
}